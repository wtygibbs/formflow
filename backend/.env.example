# Database
CONNECTION_STRING=Server=localhost;Database=AcordParserDb;Trusted_Connection=True;MultipleActiveResultSets=true

# JWT Configuration
JWT_KEY=your-256-bit-secret-key-here-change-in-production-must-be-at-least-32-characters
JWT_ISSUER=AcordParser
JWT_AUDIENCE=AcordParserAPI

# Azure Storage
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=youraccount;AccountKey=yourkey;EndpointSuffix=core.windows.net
AZURE_STORAGE_CONTAINER_NAME=documents

# Azure Document Intelligence
AZURE_DOCUMENT_INTELLIGENCE_ENDPOINT=https://your-resource.cognitiveservices.azure.com/
AZURE_DOCUMENT_INTELLIGENCE_API_KEY=your-api-key
AZURE_DOCUMENT_INTELLIGENCE_MODEL_ID=prebuilt-document

# Stripe
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Application URLs
APP_BASE_URL=https://localhost:5001
APP_FRONTEND_URL=http://localhost:4200
